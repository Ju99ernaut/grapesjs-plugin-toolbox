{
  "name": "grapesjs-plugin-toolbox",
  "version": "1.0.0",
  "description": "Grapesjs Plugin Toolbox",
  "main": "dist/grapesjs-plugin-toolbox.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ju99ernaut/grapesjs-plugin-toolbox.git"
  },
  "scripts": {
    "start": "grapesjs-cli serve",
